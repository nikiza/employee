class Employee
  attr_accessor :name, :age, :yearly_salary, :gender

  def age_by_one_year
    puts "#{age} + 1"
  end

end

employee = Employee.new
employee.name = "Hugo"
employee.age = "45"
employee.yearly_salary = "50000"
employee.gender = "male"

if age_by_one_year == 65
  puts "Employee is ready for retirement"
elseif
  puts "There are (65 - #{age}) years left until retirement."
end
